/**app.wxss**/
@import "wxParse/wxParse.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  font-size: 28rpx;
  width:100%;
  overflow-x: hidden;
} 

.icon {
  display: inline-block;
  width:20px;
  height: 20px;
  vertical-align: bottom;
  margin:0 15rpx;
  transform: scale(1.5);
  
}

button{
  background:#20AA39;
  color:#fff;
}

page{
  background-color:#fff;
  width:100%;
}
